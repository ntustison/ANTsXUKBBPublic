	mesh = {Betacoronavirus; Bias; COVID-19; Coronavirus Infections; Humans; Observational Studies as Topic; Pandemics; Pneumonia, Viral; Risk Factors; SARS-CoV-2; Treatment Outcome},
	month = {11},
