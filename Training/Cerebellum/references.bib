	keywords = {Automated segmentation; Brain MRI; Neurodegeneration; Vascular pathology; White matter hyperintensities},
	mesh = {Aged; Aged, 80 and over; Algorithms; Brain; Diffusion Tensor Imaging; Female; Humans; Image Enhancement; Image Interpretation, Computer-Assisted; Leukoaraiosis; Male; Middle Aged; Pattern Recognition, Automated; Reproducibility of Results; Sensitivity and Specificity; White Matter},
	month = {Nov},
	pages = {191-205},
	pmc = {PMC5035138},
	pmid = {27402600},
