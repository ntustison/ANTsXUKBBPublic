	doi = {10.1016/j.neurobiolaging.2018.05.023},
	journal = {Neurobiol Aging},
	journal-full = {Neurobiology of aging},
	keywords = {APOE; Alzheimer's disease; High-resolution fMRI; Hippocampus; Pattern separation},
	mesh = {African Americans; Aged; Alzheimer Disease; Apolipoprotein E4; Brain Mapping; Case-Control Studies; Discrimination, Psychological; Female; Genetic Predisposition to Disease; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Middle Aged},
	month = {09},
	pages = {221-229},
	pmc = {PMC6392014},
	pmid = {29909179},
	pst = {ppublish},
	title = {APOE $\varepsilon$4 status in healthy older African Americans is associated with deficits in pattern separation and hippocampal hyperactivation},
	volume = {69},
	year = {2018},
	bdsk-url-1 = {https://doi.org/10.1016/j.neurobiolaging.2018.05.023}}

@article{Brown:2019aa,
	abstract = {In animals, stress and corticosteroid excess are associated with decreases in memory performance and hippocampal volume that may be prevented with agents that decrease glutamate release. Humans also demonstrate changes in memory and hippocampus with corticosteroids. In this report the effects of glutamate-release inhibitor lamotrigine on hippocampal structure and memory were examined in people receiving medically needed prescription corticosteroid therapy. A total of 54 outpatient adults (n = 28 women) receiving chronic (≥ 6 months) oral corticosteroid therapy were randomized to lamotrigine or placebo for 48 weeks. Declarative memory was assessed using the Rey Auditory Verbal Learning Test (RAVLT); structural magnetic resonance imaging (MRI) as well as single-voxel proton MR spectroscopy (1HMRS) focused on hippocampus were obtained at baseline and week 48. Utilizing a mixed-model approach, structural and biochemical data were examined by separate ANOVAs, and memory was assessed with a multi-level longitudinal model. RAVLT total scores demonstrated significantly better declarative memory performance with lamotrigine than placebo (p = 0.047). Hippocampal subfield volumes were not significantly different between the treatment groups. In summary, lamotrigine was associated with less decline in declarative memory performance than placebo in corticosteroid-treated patients. Findings suggest that, in humans as well as in animal models, glutamate release inhibitors may attenuate some of the effects on the human memory associated with corticosteroids.},
	author = {Brown, E Sherwood and Sayed, Nasreen and Choi, Changho and Tustison, Nicholas and Roberts, Jared and Yassa, Michael A and Van Enkevort, Erin and Nakamura, Alyson and Ivleva, Elena I and Sunderajan, Prabha and Khan, David A and Vazquez, Miguel and McEwen, Bruce and Kulikova, Alexandra and Frol, Alan B and Holmes, Traci},
	date-added = {2021-11-29 18:02:55 -0800},
	date-modified = {2022-02-01 13:31:42 -0800},
	doi = {10.1016/j.euroneuro.2018.12.012},
	journal = {Eur Neuropsychopharmacol},
	journal-full = {European neuropsychopharmacology : the journal of the European College of Neuropsychopharmacology},
	keywords = {Corticosteroid; Hippocampus; Magnetic resonance imaging; Memory; Prednisone},
	mesh = {Adolescent; Adrenal Cortex Hormones; Adult; Aged; Antipsychotic Agents; Double-Blind Method; Female; Functional Laterality; Glutamic Acid; Hippocampus; Humans; Lamotrigine; Magnetic Resonance Imaging; Magnetic Resonance Spectroscopy; Male; Memory Disorders; Middle Aged; Young Adult},
	month = {03},
	number = {3},
	pages = {376-383},
	pmid = {30612854},
	pst = {ppublish},
	title = {A randomized, double-blind, placebo-controlled trial of lamotrigine for prescription corticosteroid effects on the human hippocampus},
	volume = {29},
	year = {2019},
	bdsk-url-1 = {https://doi.org/10.1016/j.euroneuro.2018.12.012}}

@article{Brown:2019vv,
	abstract = {Preclinical and clinical research indicates that excess corticosteroid is associated with adverse effects on the hippocampus. Animal model data suggest that N-methyl-D-aspartate (NMDA) receptor antagonists may block corticosteroid effect on the hippocampus. This translational clinical trial investigated the effect of memantine vs. placebo on hippocampal subfield volume in humans receiving chronic corticosteroid therapy. Men and women (N = 46) receiving chronic prescription corticosteroid therapy were randomized to memantine or placebo in a double-blind, crossover design (two 24-week treatment periods, separated by a 4-week washout) for 52 weeks. Structural magnetic resonance imaging was obtained at baseline and after each treatment. Data were analyzed using repeated measures analysis of variance. Mean corticosteroid dose was 7.69 $\pm$ 6.41 mg/day and mean duration 4.90 $\pm$ 5.61 years. Controlling for baseline volumes, the left DG/CA3 region was significantly larger following memantine than placebo (p = .011). The findings suggest that an NMDA receptor antagonist attenuates corticosteroid effect in the same hippocampal subfields in humans as in animal models. This finding has both mechanistic and clinical implications. Attenuation of the effect of corticosteroids on the human DG/CA3 region implicates the NMDA receptor in human hippocampal volume losses with corticosteroids. In addition, by suggesting a drug class that may, at least in part, block the effects of corticosteroids on the human DG/CA3 subfield, these results may have clinical relevance for people receiving prescription corticosteroids, as well as to those with cortisol elevations due to medical or psychiatric conditions.},
	author = {Brown, E Sherwood and Kulikova, Alexandra and Van Enkevort, Erin and Nakamura, Alyson and Ivleva, Elena I and Tustison, Nicholas J and Roberts, Jared and Yassa, Michael A and Choi, Changho and Frol, Alan and Khan, David A and Vazquez, Miguel and Holmes, Traci and Malone, Kendra},
	date-added = {2021-11-29 18:01:00 -0800},
	date-modified = {2021-11-29 18:01:00 -0800},
	doi = {10.1038/s41386-019-0430-8},
	journal = {Neuropsychopharmacology},
	journal-full = {Neuropsychopharmacology : official publication of the American College of Neuropsychopharmacology},
	mesh = {Adrenal Cortex Hormones; Adult; Cross-Over Studies; Double-Blind Method; Female; Hippocampus; Humans; Magnetic Resonance Imaging; Male; Memantine; Middle Aged; Receptors, N-Methyl-D-Aspartate; Treatment Outcome},
	month = {12},
	number = {13},
	pages = {2263-2267},
	pmc = {PMC6898191},
	pmid = {31181564},
	pst = {ppublish},
	title = {A randomized trial of an NMDA receptor antagonist for reversing corticosteroid effects on the human hippocampus},
	volume = {44},
	year = {2019},
	bdsk-url-1 = {https://doi.org/10.1038/s41386-019-0430-8}}

@article{Nguyen:2019aa,
	abstract = {PURPOSE/BACKGROUND: Glucocorticoids are a class of hormones that include naturally occurring cortisol and corticosterone, as well as prescription drugs commonly used to manage inflammatory, autoimmune, and allergic conditions. Adverse effects, including neuropsychiatric symptoms, are common. The hippocampus appears to be especially sensitive to the effects of glucocorticoids. However, to our knowledge, no studies to date have examined hippocampal subfields in humans receiving glucocorticoids. We examined patients on chronic glucocorticoid regimens to determine relationships between dose and duration of treatment, and hippocampal subfields, and related regions volumes.
METHODS/PROCEDURES: The study included adult men and women receiving at least 5 mg daily of prednisone equivalents for at least 6 months. Volumes of brain regions were measured via magnetic resonance imaging. A multivariate general linear model was used for analysis, with brain volumes as dependent variables and age, sex, and cumulative corticosteroid exposure, as predictors.
FINDINGS/RESULTS: The study population consisted of 81 adult outpatients (43 male) on corticosteroids (mean dose, 7.88 mg; mean duration, 76.75 months). Cumulative glucocorticoid exposure was negatively associated with left and right hippocampal dentate gyrus/CA3 volume. In subsequent subgroup analysis, this association held true for the age group older than the median age of 46 years but not for the younger age group.
IMPLICATIONS/CONCLUSIONS: This finding is consistent with previous studies showing detrimental effects of elevated glucocorticoids on the hippocampus but further suggests that the dentate gyrus and CA3 regions are particularly vulnerable to those effects, which is consistent with animal models of chronic stress but has not been previously demonstrated in humans.},
	author = {Nguyen, Duc M and Yassa, Michael A and Tustison, Nicholas J and Roberts, Jared M and Kulikova, Alexandra and Nakamura, Alyson and Ivleva, Elena I and Van Enkevort, Erin and Brown, E Sherwood},
	date = {2019 Nov/Dec},
	date-added = {2021-11-29 18:00:25 -0800},
	date-modified = {2022-02-01 13:33:19 -0800},
	doi = {10.1097/JCP.0000000000001120},
	journal = {J Clin Psychopharmacol},
	journal-full = {Journal of clinical psychopharmacology},
	mesh = {Adult; Aged; CA3 Region, Hippocampal; Clinical Trials as Topic; Dentate Gyrus; Female; Glucocorticoids; Humans; Magnetic Resonance Imaging; Male; Middle Aged; Neuroimaging; Prednisone; Young Adult},
	number = {6},
	pages = {653-657},
	pmc = {PMC6856429},
	pmid = {31688386},
	pst = {ppublish},
	title = {The Relationship Between Cumulative Exogenous Corticosteroid Exposure and Volumes of Hippocampal Subfields and Surrounding Structures},
	volume = {39},
	year = {2019},
	bdsk-url-1 = {https://doi.org/10.1097/JCP.0000000000001120}}

@article{McMakin:2021vl,
	abstract = {This study examines neural mechanisms of negative overgeneralization, the increased likelihood of generalizing negative information, in peri-puberty. Theories suggest that weak pattern separation (overlapping representations are made distinct, indexed by DG/CA3 hippocampal subfield activation) underlies negative overgeneralization. We alternatively propose that neuro-maturational changes that favor pattern completion (cues reinstate stored representations, indexed by CA1 activation) are modulated by circuitry involved in emotional responding (amygdala, medial prefrontal cortices [mPFC]) to drive negative overgeneralization. Youth (N=34, 9-14 years) recruited from community and clinic settings participated in an emotional mnemonic similarity task while undergoing MRI. At Study, participants indicated the valence of images; at Test, participants made recognition memory judgments. Critical lure stimuli, that were similar to images at Study, were presented at Test, and errors ("false alarms") to negative relative to neutral stimuli reflected negative overgeneralization. Negative overgeneralization was related to greater and more similar patterns of activation in CA1 and both dorsal and ventral mPFC for negative relative to neutral stimuli. At Study, amygdala exhibited greater functional coupling with CA1 and dorsal mPFC during negative items that were later generalized. Negative overgeneralization is rooted in amygdala and mPFC modulation at encoding and pattern completion at retrieval.},
	author = {McMakin, Dana L and Kimbler, Adam and Tustison, Nicholas J and Pettit, Jeremy W and Mattfeld, Aaron T},
	date-added = {2021-11-29 17:59:41 -0800},
	date-modified = {2021-11-29 17:59:41 -0800},
	doi = {10.1093/scan/nsab089},
	journal = {Soc Cogn Affect Neurosci},
	journal-full = {Social cognitive and affective neuroscience},
	keywords = {anxiety; fMRI; generalization; hippocampus; mPFC; pattern completion},
	month = {Jul},
	pmid = {34270763},
	pst = {aheadofprint},
	title = {Negative Overgeneralization is Associated with Anxiety and Mechanisms of Pattern Completion in Peripubertal Youth},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.1093/scan/nsab089}}

@article{Foo:2021vi,
	abstract = {Hippocampal volume is an important biomarker of Alzheimer's disease (AD), and genetic risk of AD is associated with hippocampal atrophy. However, the hippocampus is not a uniform structure and has a number of subfields, the associations of which with age, sex, and polygenic risk score for AD (PRSAD) have been inadequately investigated. We examined these associations in 17,161 cognitively normal UK Biobank participants (44-80 years). Age was negatively associated with all the hippocampal subfield volumes and females had smaller volumes than men. Higher PRSAD was associated with lower volumes in the bilateral whole hippocampus, hippocampal-amygdala-transition-area, and hippocampal tail; right subiculum; left cornu ammonis 1, cornu ammonis 4, molecular layer, and granule cell layer of dentate gyrus. Older individuals (median age 63 years, n = 8984) showed greater subfield vulnerability to high PRSAD compared to the younger group (n = 8177), but the effect did not differ by sex. The pattern of subfield involvement in relation to the PRSAD in community dwelling healthy individuals sheds additional light on the pathogenesis of AD.},
	author = {Foo, Heidi and Thalamuthu, Anbupalam and Jiang, Jiyang and Koch, Forrest and Mather, Karen A and Wen, Wei and Sachdev, Perminder S},
	date-added = {2021-11-29 17:57:43 -0800},
	date-modified = {2021-11-29 17:57:43 -0800},
	doi = {10.1016/j.neurobiolaging.2020.11.002},
	journal = {Neurobiol Aging},
	journal-full = {Neurobiology of aging},
	keywords = {Aging; Alzheimer's disease; Hippocampal subfields; Polygenic risk score},
	mesh = {Adult; Aged; Aged, 80 and over; Aging; Alzheimer Disease; Biological Specimen Banks; Female; Genetic Association Studies; Hippocampus; Humans; Independent Living; Male; Middle Aged; Multifactorial Inheritance; Organ Size; Risk; United Kingdom},
	month = {02},
	pages = {108-115},
	pmid = {33259984},
	pst = {ppublish},
	title = {Associations between Alzheimer's disease polygenic risk scores and hippocampal subfield volumes in 17,161 UK Biobank participants},
	volume = {98},
	year = {2021},
	bdsk-url-1 = {https://doi.org/10.1016/j.neurobiolaging.2020.11.002}}

