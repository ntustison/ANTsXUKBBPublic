	mesh = {Algorithms; Brain Neoplasms; Humans; Image Interpretation, Computer-Assisted; Models, Theoretical; Pattern Recognition, Automated; Reproducibility of Results; Sensitivity and Specificity},
	month = {Apr},
	number = {2},
	pages = {209-25},
	pmid = {25433513},
	pst = {ppublish},
	title = {Optimal Symmetric Multimodal Templates and Concatenated Random Forests for Supervised Brain Tumor Segmentation (Simplified) with ANTsR},
	volume = {13},
	year = {2015},
